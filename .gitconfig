    ui = auto

[alias]
    s = status
    c = commit
    ca = commit --amend
    l = log
    b = branch
    co = checkout

[github]
    user = sovannara-hak

[push]
    default = simple

[url "git@github.com:sovannara-hak/"]
    pushInsteadOf = https://github.com/sovannara-hak/

[url "git@github.com:ISIR-SYROCO/"]
    pushInsteadOf = https://github.com/ISIR-SYROCO/
